    <string name="gyroscope_cal">"Sự định cở con quay hồi chuyển"</string>
    <string name="gyroscope_cal_intro">"Đặt thiết bị lên bàn và để yên đó. Sau đó ấn nút bắt đầu để bắt đầu hiệu chỉnh."</string>
    <string name="gyroscope_cal_start">"bắt đầu điều chỉnh"</string>
    <string name="gyroscope_cal_alert_title">"kết quả"</string>
    <string name="gyroscope_cal_alert_ok_btn">"Xác nhận"</string>
    <string name="gyroscope_cal_alert_success">"Điều chỉnh thành công!"</string>
    <string name="gyroscope_cal_alert_fail">"Điều chỉnh thất bại !"</string>

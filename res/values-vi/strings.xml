<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="sensor_cal">"Cảm biến hiệu chỉnh"</string>
    <string name="compass_cal">"La bàn hiệu chỉnh"</string>
    <string name="compass_cal_title">"Điều chỉnh la bàn"</string>
    <string name="compass_cal_start">"bắt đầu điều chỉnh"</string>
    <string name="compass_cal_intro" product="tablet">"Thông qua điều chỉnh bộ phận la bàn trên máy tính bảng để đạt được dữ liệu la bàn chính xác nhất. Lần lượt xoay máy tính bảng xung quanh 3 trục. Lập lại những động tác trên cho đến khi tin báo thành công hiện ra.
            \n\nBạn đã sẵn sàng chưa ? hãy ấn nút bắt đầu." </string>
    <string name="compass_cal_intro" product="default">"Thông qua điều chỉnh bộ phận la bàn trên điện thoại để đạt được dữ liệu la bàn chính xác nhất. Lần lượt xoay điện thoại xung quanh 3 trục. Lập lại những động tác trên cho đến khi tin báo thành công hiện ra.
            \n\nBạn đã sẵn sàng chưa ? hãy ấn nút bắt đầu." </string>
    <string name="compass_cal_alert_title">"kết quả"</string>
    <string name="compass_cal_alert_ok_btn">"Xác nhận"</string>

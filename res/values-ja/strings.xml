    <string name="gyroscope_cal_start">"較正を開始"</string>
    <string name="gyroscope_cal_alert_title">"結果"</string>
    <string name="gyroscope_cal_alert_ok_btn">OK</string>
    <string name="gyroscope_cal_alert_success">"較正に成功しました！"</string>
    <string name="gyroscope_cal_alert_fail">"較正失敗しました！"</string>

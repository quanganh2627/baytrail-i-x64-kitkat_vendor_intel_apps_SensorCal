<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="sensor_cal">Sensor calibration</string>
    <string name="board_type" product="tablet">tablet</string>
    <string name="board_type" product="default">phone</string>
    <string name="compass_cal">Compass calibration</string>
    <string name="compass_cal_title">Calibrate your compass</string>
    <string name="compass_cal_start">Start calibration</string>
    <string name="compass_cal_intro" product="tablet">Get most accurate compass data by calibrating the compass device in the tablet. Wave the tablet in a 3D figure 8 motion. Repeat the wave motion till the success message pops out.
        \n\nReady to start? Touch Start calibration.
    </string>
    <string name="compass_cal_intro" product="default">Get most accurate compass data by calibrating the compass device in the phone. Wave the phone in a 3D figure 8 motion. Repeat the wave motion till the success message pops out.
        \n\nReady to start? Touch Start calibration.
    </string>
    <string name="compass_cal_alert_title">Result</string>
    <string name="compass_cal_alert_ok_btn">OK</string>
    <string name="compass_cal_alert_success">Calibration Succeeded!</string>
    <string name="result_text">Calibration Accuracy</string>

    <string name="gyroscope_cal">Gyroscope calibration</string>
    <string name="gyroscope_cal_intro" product="tablet">Put the tablet on a table and do not move it. Then press the Start calibration button to do the calibration.</string>
    <string name="gyroscope_cal_intro" product="default">Put the phone on a table and do not move it. Then press the Start calibration button to do the calibration.</string>
    <string name="gyroscope_cal_start">Start calibration</string>
    <string name="gyroscope_cal_alert_title">Result</string>
    <string name="gyroscope_cal_alert_ok_btn">OK</string>
    <string name="gyroscope_cal_alert_success">Calibration Succeeded!</string>
    <string name="gyroscope_cal_alert_fail">Calibration Failed!</string>
</resources>
